{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ES5",
    "baseUrl": "./",
    "composite": true,
    "strictNullChecks": true,
    "declaration": true,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "include": ["./src"],
  "exclude": ["./dist"]
}
